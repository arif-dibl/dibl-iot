# Coolify Environment Variables Template
# Copy these values to Coolify environment settings

# REQUIRED: Domain (e.g. abc.1.2.3.4.sslip.io or custom domain)
OR_HOSTNAME=

# REQUIRED: Admin password
OR_ADMIN_PASSWORD=

# OPTIONAL
OR_EMAIL_ADMIN=
OR_SSL_PORT=-1
OR_DEV_MODE=false
OR_METRICS_ENABLED=true

# Docker images
PROXY_VERSION=latest
POSTGRESQL_VERSION=latest
KEYCLOAK_VERSION=latest
MANAGER_VERSION=latest
